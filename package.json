{
  "name": "npmkit-website",
  "version": "1.0.0",
  "description": "Website for npmkit app",
  "main": "./pages/index.js",
  "repository": "https://github.com/sbekrin/npmkit.app",
  "homepage": "https://npmkit.app",
  "author": "Sergey Bekrin <sergey@bekrin.me> https://bekrin.me",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next start"
  },
  "dependencies": {
    "next": "^7.0.2",
    "node-fetch": "^2.3.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3",
    "system-font-css": "^2.0.2"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "raw-loader": "^1.0.0",
    "serve": "^10.1.1"
  },
  "prettier": {
    "singleQuote": true,
    "proseWrap": "always",
    "trailingComma": "all"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
